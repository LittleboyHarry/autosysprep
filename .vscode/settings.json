{
    "files.associations": {
        "*.reg": "ini",
        "**/lib/newdesktop/*.txt": "markdown",
        "**/features/customPwshProfile/GitToolsLoader/aliases.macro.txt": "shellscript"
    },
    "search.exclude": {
        "**/lib/modules/master/PoshRSJob": true
    },
    "[ini]": {
        "files.encoding": "utf16le",
        "editor.wordWrap": "off"
    },
    "[powershell]": {
        "files.encoding": "utf16le",
        "editor.wordWrap": "off"
    },
    "[md]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
